{
  "name": "@pctx/sdk",
  "version": "0.1.0",
  "description": "Node.js SDK for PCTX - complete toolkit for MCP servers and local tools",
  "main": "index.js",
  "types": "index.d.ts",
  "napi": {
    "name": "pctx",
    "triples": {
      "additional": [
        "aarch64-apple-darwin",
        "x86_64-apple-darwin",
        "x86_64-pc-windows-msvc",
        "x86_64-unknown-linux-gnu",
        "aarch64-unknown-linux-gnu"
      ]
    }
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "prepublishOnly": "napi prepublish -t npm",
    "test": "ava",
    "version": "napi version"
  },
  "keywords": [
    "pctx",
    "mcp",
    "ai",
    "agents",
    "tools",
    "typescript",
    "model-context-protocol"
  ],
  "author": "Port of Context",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/portofcontext/pctx.git"
  },
  "devDependencies": {
    "@napi-rs/cli": "^2.18.0"
  },
  "engines": {
    "node": ">= 16"
  }
}
